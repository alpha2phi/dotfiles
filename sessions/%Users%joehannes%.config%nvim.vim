badd +165 ~/.config/nvim/lua/keymappings.lua
badd +21 ~/.config/nvim/lua/lang/init.lua
argglobal
%argdel
$argadd ~/.config/nvim/init.lua
edit ~/.config/nvim/lua/keymappings.lua
